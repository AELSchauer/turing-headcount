DirectRepository
	#find_by_name
		-input : district name (not case sensitive)
		-output : <District> or nil
	#find_all_matching
		-input : name fragment (start of name only?)
		-output : Array of <Districts> or []
	#load_data
		-input : nested hash
		-output : nil
		-automatically creates <EnrollmentRepository> and <Enrollment> objects
	*data = hash w/ district name as string is key and <District> is value

District
	#new/initialize
		-input : hash (see documentation)
		-output : nil
	*name = upcase name of <District>
	*enrollment = <Enrollment>
	*economic_profile = <EconomicProfile>
	*statewide_test = <StatewideTest>


EnrollmentRepository/EconomicProfileRepository/StatewideTestRepository
	#find_by_name
		-input : district name (not case sensitive)
		-output : <Enrollment>/<EconomicProfile>/<StatewideTest> or nil
	#load_data
		-input : nested hash
		-output : nil
	DataRepository = Module

Enrollment/EconomicProfile/StatewideTest
	#new/initialize
		-input : hash (see documentation)
		-output : nil
	*data = nested hash w/ data
	*name = upcase name of <Enrollment>
	Data = Module
	---
	Enrollment Only
		#kindergarten_participation_by_year
			-input : none
			-output : hash
		#kindergarten_participation_in_year
			-input : year
			-output : 3d floating decimal number (e.g. 0.391)
	---
	EconomicProfile Only
		#median_household_income_in_year
		#median_household_income_average
		#children_in_poverty_in_year
		#free_or_reduced_price_lunch_percentage_in_year
		#free_or_reduced_price_lunch_number_in_year
		#title_i_in_year
	---
	StatewideTest Only
		#proficient_by_grade
		#proficient_by_race_or_ethnicity
		#proficient_for_subject_by_grade_in_year
		#proficient_for_subject_by_race_in_year


HeadcountAnalysis
	#new/initialize
		-input : <DirectRepository>
		-output : nil
	#kindergarten_participation_rate_variation
		-input : district name string, hash {:against => comparison district name string}
		-process : see documentation
		-output : 3d floating decimal number (e.g. 0.391)
	#kindergarten_participation_rate_variation_trend
		-input : district name string, hash {:against => comparison district name string}
		-process : see documentation
		-output : hash
	#kindergarten_participation_against_high_school_graduation
	#kindergarten_participation_correlates_with_high_school_graduation
	#kindergarten_participation_against_household_income
	#top_statewide_test_year_over_year_growth


ResultSet
	#matching_districts
		-output : array
	#statewide_average


ResultEntry
	*name
	*free_and_reduced_price_lunch_rate
	*children_in_poverty_rate
	*high_school_graduation_rate
	*median_household_income



